.wux-loading{position:fixed;z-index:5000;top:50%;left:50%;background:rgba(40,40,40,.75);font-size:28rpx;line-height:1.5;text-align:center;border-radius:6rpx;color:#fff;transform:translate3d(-50%,-50%,0)}.wux-loading__content{min-width:236rpx;padding:18rpx 30rpx;box-sizing:border-box}.wux-loading__content--has-icon{padding:30rpx}.wux-loading__content--has-icon .wux-loading__text{margin-top:12rpx}.wux-loading__icon{margin:0;display:block}.wux-loading__icon--loading{display:inline-block!important;vertical-align:middle;animation:loading 1s steps(12,end) infinite;background:transparent url(data:image/svg+xml;base64,) no-repeat;background-size:100%;margin:10rpx 0 0!important;width:76rpx;height:76rpx;vertical-align:baseline}@keyframes loading{0%{transform:rotate3d(0,0,1,0deg)}100%{transform:rotate3d(0,0,1,360deg)}}